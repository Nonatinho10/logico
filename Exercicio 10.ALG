Algoritmo "semnome"


var
NumA, NumB, troca: real

inicio

escreval ("Informe o valor da variável NumA: ")

leia (NumA)

escreval ("Informe o valor da variável NumB: ")

leia (NumB)

troca <- NumA

NumA <- NumB

NumB <- troca

escreval ("O novo valor de NumA é: ", NumA)

escreval ("O novo valor de NumB é: ", NumB)

fimalgoritmo